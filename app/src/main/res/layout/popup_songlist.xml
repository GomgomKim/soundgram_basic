<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/popup_element"
    android:layout_width="match_parent"
    android:layout_height="350dp"
    android:padding="10dp"
    android:background="@drawable/play_ic_popup"
    android:orientation="vertical">


   <LinearLayout
       android:paddingLeft="30dp"
       android:layout_marginTop="30dp"
       android:layout_marginLeft="10dp"
       android:layout_marginBottom="10dp"
       android:orientation="horizontal"
       android:layout_width="match_parent"
       android:layout_height="wrap_content">

      <ImageView
          android:layout_width="30dp"
          android:layout_height="30dp"
          android:layout_gravity="center"
          android:src="@drawable/play_ic_cd"
          android:layout_marginRight="10dp"/>
      <TextView
          android:textColor="#bdbdbd"
          android:text="01     "
          android:layout_gravity="center"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" />

      <TextView
          android:id="@+id/music1"
          android:textColor="#5d5d5d"
          android:text="애월"
          android:layout_gravity="center"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" />
   </LinearLayout>


   <LinearLayout
       android:paddingLeft="30dp"
       android:layout_margin="10dp"
       android:orientation="horizontal"
       android:layout_width="match_parent"
       android:layout_height="wrap_content">
      <ImageView
          android:layout_width="30dp"
          android:layout_height="30dp"
          android:src="@drawable/play_ic_cd"
          android:layout_marginRight="10dp"/>
      <TextView
          android:textColor="#bdbdbd"
          android:text="02     "
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center"
          />

      <TextView
          android:id="@+id/music2"
          android:textColor="#5d5d5d"
          android:text="모네"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center"
          />
   </LinearLayout>


   <LinearLayout
       android:paddingLeft="30dp"
       android:layout_margin="10dp"
       android:orientation="horizontal"
       android:layout_width="match_parent"
       android:layout_height="wrap_content">
      <ImageView
          android:layout_width="30dp"
          android:layout_height="30dp"
          android:src="@drawable/play_ic_cd"
          android:layout_marginRight="10dp"/>
      <TextView
          android:textColor="#bdbdbd"
          android:text="03     "
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center"
          />

      <TextView
          android:id="@+id/music3"
          android:textColor="#5d5d5d"
          android:text="FULL"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center"
          />
   </LinearLayout>


   <LinearLayout
       android:paddingLeft="30dp"
       android:layout_margin="10dp"
       android:orientation="horizontal"
       android:layout_width="match_parent"
       android:layout_height="wrap_content">
      <ImageView
          android:layout_width="30dp"
          android:layout_height="30dp"
          android:src="@drawable/play_ic_cd"
          android:layout_marginRight="10dp"/>
      <TextView
          android:textColor="#bdbdbd"
          android:text="04     "
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center"
          />

      <TextView
          android:id="@+id/music4"
          android:textColor="#5d5d5d"
          android:text="비행운"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center"
          />
   </LinearLayout>


   <LinearLayout
       android:paddingLeft="30dp"
       android:layout_margin="10dp"
       android:orientation="horizontal"
       android:layout_width="match_parent"
       android:layout_height="wrap_content">
      <ImageView
          android:layout_width="30dp"
          android:layout_height="30dp"
          android:src="@drawable/play_ic_cd"
          android:layout_marginRight="10dp"/>
      <TextView
          android:textColor="#bdbdbd"
          android:text="05     "
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center"
          />

      <TextView
          android:id="@+id/music5"
          android:textColor="#5d5d5d"
          android:text="디왈리 (With 저수지의 딸들)"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:layout_gravity="center"
          />
   </LinearLayout>


   <LinearLayout
       android:paddingLeft="30dp"
       android:layout_margin="10dp"
       android:orientation="horizontal"
       android:visibility="gone"
       android:layout_width="match_parent"
       android:layout_height="wrap_content">
      <ImageView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:src="@drawable/play_ic_cd"
          android:layout_marginRight="10dp"/>
      <TextView
          android:textColor="#bdbdbd"
          android:text="06     "
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" />

      <TextView
          android:id="@+id/music6"
          android:textColor="#5d5d5d"
          android:text="무릎"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" />
   </LinearLayout>


   <LinearLayout
       android:paddingLeft="30dp"
       android:layout_margin="10dp"
       android:orientation="horizontal"
       android:visibility="gone"
       android:layout_width="match_parent"
       android:layout_height="wrap_content">
      <ImageView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"
          android:src="@drawable/play_ic_cd"
          android:layout_marginRight="10dp"/>
      <TextView
          android:textColor="#bdbdbd"
          android:text="07     "
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" />

      <TextView
          android:id="@+id/music7"
          android:textColor="#5d5d5d"
          android:text="안경"
          android:layout_width="wrap_content"
          android:layout_height="wrap_content" />
   </LinearLayout>



</LinearLayout>